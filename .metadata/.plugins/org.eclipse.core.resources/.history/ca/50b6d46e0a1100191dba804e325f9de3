#include "nrf24.h"

//------------------------------------------------

extern SPI_HandleTypeDef hspi1;

//------------------------------------------------

__STATIC_INLINE void DelayMicro(__IO uint32_t micros)

{

  micros *= (SystemCoreClock / 1000000) / 9;

  /* Wait till done */

  while (micros--) ;

}

void NRF24_ini(void)
{

}
